<h1>Welcome to Applican!</h1>
<h2>Please make a new account</h2>

<%= form_for(@user) do |f| %>
    <%= f.label "First Name" %>
    <%= f.text_field :first_name %><br><br>
    <%= f.label "Last Name" %>
    <%= f.text_field :last_name %><br><br>
    <%= f.label "Email: " %>
    <%= f.text_field :email %><br><br>
    <%= f.label "LinkedIn URL: " %>
    <%= f.text_field :linkedin%><br><br>
    <%= f.label "Github URL: " %>
    <%= f.text_field :github %><br><br>
    <%= f.submit "Finished" %>
<% end %>
